local Button = Server:CreateButton({
   Name = "Stop time",
   Callback = function()
   local Players = game:GetService("Players")
local Lighting = game:GetService("Lighting")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")

-- >>> SETTINGS <<<
local executorUserName = "YourUserNameHere" -- Replace with the username of the player who can move
local freezeDuration = 10 -- seconds
local clockModelName = "ClockModel" -- a clock model stored in ReplicatedStorage

-- >>> FREEZE PLAYERS <<<
local function freezePlayer(player)
	if player.Character then
		local hum = player.Character:FindFirstChildOfClass("Humanoid")
		if hum then
			hum.WalkSpeed = 0
			hum.JumpHeight = 0
			hum.PlatformStand = true
		end
	end
end

local function unfreezePlayer(player)
	if player.Character then
		local hum = player.Character:FindFirstChildOfClass("Humanoid")
		if hum then
			hum.WalkSpeed = 16
			hum.JumpHeight = 7.2
			hum.PlatformStand = false
		end
	end
end

-- >>> SET MAP GREY <<<
local originalAmbient = Lighting.Ambient
local originalOutdoorAmbient = Lighting.OutdoorAmbient

local function setMapGrey()
	Lighting.Ambient = Color3.fromRGB(169, 169, 169)
	Lighting.OutdoorAmbient = Color3.fromRGB(169, 169, 169)
end

local function resetMapColor()
	Lighting.Ambient = originalAmbient
	Lighting.OutdoorAmbient = originalOutdoorAmbient
end

-- >>> CLOCK EFFECT <<<
local function spawnClock()
	local clockTemplate = ReplicatedStorage:FindFirstChild(clockModelName)
	if clockTemplate then
		local clock = clockTemplate:Clone()
		clock.Parent = Workspace
		clock:SetPrimaryPartCFrame(CFrame.new(0, 10, 0)) -- position it above the map
		game.Debris:AddItem(clock, freezeDuration) -- automatically remove after freezeDuration
	end
end

-- >>> MAIN EVENT <<<
spawnClock()        -- spawn the clock model
setMapGrey()        -- make map grey

for _, player in pairs(Players:GetPlayers()) do
	if player.Name ~= executorUserName then
		freezePlayer(player)
	end
end

task.wait(freezeDuration)

resetMapColor()

for _, player in pairs(Players:GetPlayers()) do
	if player.Name ~= executorUserName then
		unfreezePlayer(player)
	end
end

   end,
})

local Button = Server:CreateButton({
   Name = "Spin all",
   Callback = function()
   local Players = game:GetService("Players")
local SpinDuration = 5       -- how long to spin (seconds)
local SpinSpeed = 10         -- degrees per frame

local function spinCharacter(character)
    local rootPart = character:FindFirstChild("HumanoidRootPart")
    if rootPart then
        -- Create a BodyAngularVelocity to spin
        local bav = Instance.new("BodyAngularVelocity")
        bav.AngularVelocity = Vector3.new(0, math.rad(SpinSpeed), 0)
        bav.MaxTorque = Vector3.new(0, math.huge, 0)
        bav.P = 1250
        bav.Parent = rootPart

        -- Remove after SpinDuration
        game.Debris:AddItem(bav, SpinDuration)
    end
end

local function spinAllPlayers()
    for _, player in ipairs(Players:GetPlayers()) do
        if player.Character then
            spinCharacter(player.Character)
        end
    end
end

-- Spin all now
spinAllPlayers()

-- Also spin players who respawn during spin time
Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function(char)
        spinCharacter(char)
    end)
end)

   end,
})

